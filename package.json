{
  "name": "react-code-icons",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./icons/*": {
      "types": "./dist/icons/*.d.ts",
      "import": "./dist/icons/*.mjs",
      "require": "./dist/icons/*.cjs",
      "default": "./dist/icons/*.mjs"
    },
    "./dynamic/*": {
      "types": "./dist/dynamic/*.d.ts",
      "import": "./dist/dynamic/*.mjs",
      "require": "./dist/dynamic/*.cjs",
      "default": "./dist/dynamic/*.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "icons",
    "language icons",
    "file icons",
    "programming languages",
    "frameworks",
    "typescript",
    "react",
    "esm",
    "tree-shaking",
    "code",
    "devtools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/code-lang-icons.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/code-lang-icons/issues"
  },
  "homepage": "https://github.com/your-org/code-lang-icons#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.17.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "changesets": "^1.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.7.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "build:check": "pnpm clean && pnpm build && pnpm typecheck && pnpm lint",
    "ci": "pnpm clean && pnpm build && pnpm typecheck && pnpm lint",
    "dev": "tsup --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky",
    "release": "changeset version && pnpm install && pnpm build && changeset publish",
    "prepublishOnly": "pnpm build",
    "test": "echo \"(tests skipped)\"",
    "icons:gen": "tsx scripts/generate-icons.ts",
    "mappings:update": "tsx scripts/update-mappings.ts",
    "mappings:update:stdin": "tsx scripts/update-mappings.ts -",
    "postbuild:dts": "tsx scripts/postbuild-dts.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "overrides": {
    "changesets": "^1.0.2"
  }
}
